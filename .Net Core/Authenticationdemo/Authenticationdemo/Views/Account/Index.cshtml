@*@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers*@
@model Authenticationdemo.Models.User




<form asp-action="Index" asp-controller="Account" method="post">
    <br />
    <label asp-for="UserName"></label>
    <br />
    <input asp-for="UserName" placeholder="Enter Employee Name" class="form-control" />
    <br />
    <label asp-for="Password"></label>
    <br />
    <input asp-for="Password" placeholder="Enter Employee Password" class="form-control" />
    <br />
    <input type="submit" value="Submit" class="btn-primary" />
    <br />
</form>